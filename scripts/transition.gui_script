require("modules.transition")

function init(self)
	Transition:set_templates()
end

function on_message(self, message_id, message, sender)
	Transition:on_message(message_id, message, sender)
end

function on_input(self, action_id, action)
	if Transition:on_input(action_id, action) then
		return true
	end
end
